                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbar_menu" runat="server">
                <ul  id="NavMenu" class="navbar-nav mr-auto" runat="server">
                </ul>
                 <ul class="navbar-nav navbar-right">
                     
                     <li class="nav-item" data-toggle="tooltip" data-placement="bottom" title="Presupuestos">
                          <asp:HyperLink ID="hlinkPresupuesto" runat="server" class="nav-link pt-2" CausesValidation="false" NavigateUrl="~/Reportes/Presupuesto.aspx"
